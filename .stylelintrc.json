{
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ],
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "order/order": [
        "custom-properties",
        "dollar-variables",
        "at-variables",
        "declarations",
        "at-rules",
        "rules",
        "less-mixins"
      ],
      "font-family-no-missing-generic-family-keyword": 0
    }
}